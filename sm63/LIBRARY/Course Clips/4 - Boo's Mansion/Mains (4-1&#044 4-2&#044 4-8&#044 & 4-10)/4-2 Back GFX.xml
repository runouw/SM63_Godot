			count = 0;
			//
			_root.PlayMessage("Big Boo: Mwahaha! Welcome to my Mansion, Mario.  Come on up to my Balcony, IF YOU CAN GET THERE!!  I've locked all the doors that you 'humans' need to get up to my balcony. MWAHHAAHA!");
