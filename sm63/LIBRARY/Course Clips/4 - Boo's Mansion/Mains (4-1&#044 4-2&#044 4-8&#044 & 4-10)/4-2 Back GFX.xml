                <DOMSymbolInstance libraryItemName="  Course Items-PL/ In-course Items/ Transportation/ Doors/Main Door Clip">
                  <matrix>
                    <Matrix a="1.5" d="1.5" tx="250" ty="-15.2"/>
                  </matrix>
                  <transformationPoint>
                    <Point/>
                  </transformationPoint>
                  <Actionscript>
                    <script><![CDATA[onClipEvent (load) {
	doorplace = "4-6-1";
	doortype = 5;
	if (_root.Starnum == 1) {
		locked = true;
		keytype = 1;
		if (_root.lvl4keyA == true) {
			removelock = true;
		}
		// 
		openwithkey = true;
		unlockitem = "lvl4keyA";
		//                      
		doorlockfunction = function () {
			_root.Course.Char.attack = true;
			_root.Course.Char.attackFrame = "Sign";
			_root.Course.Char.xspeed = 0;
			//
			_root.PlayMessage("This Door is Locked! Maybe there's a key somewhere nearby");
			_root.Mariosound = new Sound(this);
			_root.Mariosound.attachSound("Boo");
			_root.Mariosound.start(0, 1);
			_root.Mariosound.setVolume(_root.MarioVolume);
			count = 0;
			onEnterFrame = function () {
				X = _x+_parent._x;
				_root.Course.Char._x += (X-_root.Course.Char._x)/5;
				//
				_root.playpause = true;
				//
				count++;
				if (_root.KeyZ() && count>5) {
					_root.StopMessage();
					//
					count = 0;
					onEnterFrame = function () {
						count++;
						if (count>5) {
							_root.playpause = false;
							_root.Course.Char.attack = false;
							onEnterFrame = null;
							gotoAndStop(1);
						}
					};
				}
			};
		};
		//
	}
}
]]></script>
                  </Actionscript>
