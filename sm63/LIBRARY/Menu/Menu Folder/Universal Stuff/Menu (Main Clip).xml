              <Actionscript>
                <script><![CDATA[this._name = "m";
TransitionF = function (framy) {
	//_root.DeleteFile(_parent.HL)
	if (framy == "Delete") {
		depth = this.getNextHighestDepth();
		this.attachMovie("MenuRedTrans", "MenuRedTrans", depth, {trans:true, _x:0, _y:0});
		//this.attachMovie("MenuRedTrans", "MenuRedTrans"+depth, depth, {trans:true, _x:0, _y:0});
	} else if (framy == "Play") {
		depth = this.getNextHighestDepth();
		this.attachMovie("MenuBlackTrans", "MenuBlackTrans"+depth, depth, {trans:true, _x:0, _y:0});
	} else {
		depth = this.getNextHighestDepth();
		//this.attachMovie("MenuTransition", "MenuTransition"+depth, depth, {trans:true, _x:-500, _y:75});
		this.attachMovie("MenuTransition", "MenuTransition"+depth, depth, {trans:true, _x:0, _y:200});
		//depth2 = this.getNextHighestDepth();
		//this.attachMovie("MenuTransition", "MenuTransition"+depth2, depth2, {trans:true, _x:500, _y:-75});
		GSF = function () {
			gotoAndStop(framy);
			clicked = false;
		};
	}
};
//
SpecialDeleteFile = function (file) {
	//_root.DeleteFile(file);
	//trace(file)
	//trace("target is "+_root.m.filem[file]);
	///_root.m.filem[file].Delete();
	//trace("target's frame:"+_root.m.filem[file].em._currentframe);
	_root.m.filem.HL = undefined;
	_root.m.filem[file].em.gotoAndPlay(2);
	//trace("target's frame:"+_root.m.filem[file].em._currentframe);
	//trace("HL:"+_root.m.filem.HL);
};
//
Message = function (a, color, important) {
	if (messageonscreen == undefined) {
		messageonscreen = 0;
	}
	if (messageonscreen<3 || important !== false) {
		if (color == undefined) {
			color = red;
		}
		depth = _root.m.getNextHighestDepth();
		_root.m.attachMovie("MenuMessage", "MenuMessage"+depth, depth, {_x:0, _y:200, txt:a, color:color});
		messageonscreen++;
	}
};
stop();
]]></script>
