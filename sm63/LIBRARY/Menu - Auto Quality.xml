            <DOMFrame index="0" keyMode="9728">
              <Actionscript>
                <script><![CDATA[//Mode = "On";
if (_root.AutoQuality == true) {
	Mode = "On";
} else {
	Mode = "Off";
}
al = 50;
sc = 75;
onEnterFrame = function () {
	if (this.hitTest(_root._xmouse, _root._ymouse, false)) {
		al = 100;
		sc = 100;
	} else {
		al = 50;
		sc = 75;
	}
	_xscale += (sc-_xscale)/2;
	_yscale = _xscale;
	_alpha += (al-_alpha)/2;
	//_root.var = Mode 
	if (Mode == "On") {
		_root.AutoQuality = true;
	} else {
		_root.AutoQuality = false;
	}
};
onMouseDown = function () {
	if (sc == 100) {
		if (ON.sc == 100 && OFF.sc == 100) {
			_xscale = 110;
			_yscale = 110;
			if (Mode == "On") {
				Mode = "Off";
			} else {
				Mode = "On";
			}
		}
	}
};
]]></script>
